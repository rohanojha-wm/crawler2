{
  "name": "url-monitor",
  "version": "1.0.0",
  "description": "TypeScript URL monitoring with periodic HTTP requests, SQLite storage, and Chart.js visualization",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc",
    "start": "npm run build && node dist/main.js",
    "dev": "npm run build && node dist/main.js",
    "headless": "npm run build && node dist/main-headless.js",
    "test": "echo \"No tests specified\" && exit 0",
    "clean": "rm -rf dist",
    "rebuild": "npm run clean && npm run build",
    "monitor": "npm run headless",
    "dashboard": "npm start"
  },
  "keywords": [
    "url-monitoring",
    "typescript",
    "sqlite",
    "express",
    "chart.js",
    "github-actions"
  ],
  "author": "URL Monitor",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.6",
    "csv-parser": "^3.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.17",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}